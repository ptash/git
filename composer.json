        "secure-http": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.loc"
        }
    ],
    "authors": [
        {
            "name": "Interprogma",
            "email": "info@interprogma.ru"
        }
    ],
    "require": {
        "cognitive/util/shell-exec": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "psr-4": {
            "GitFixture\\": "src/",
            "GitFixture\\Tests\\": "tests/"
        }
    }
