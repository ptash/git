{
    "name": "cognitive/git",
    "description": "Library to work with Git and to test library which using git",
    "type": "library",
    "homepage": "http://www.cognitive.ru/",
    "authors": [
        {
            "name": "Cognitive",
            "email": "info@cognitive.ru"
        }
    ],
    "config": {
        "secure-http": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.loc"
        }
    ],
    "authors": [
        {
            "name": "Interprogma",
            "email": "info@interprogma.ru"
        }
    ],
    "require": {
        "cognitive/shell-exec": "dev-master",
        "cognitive/file-system": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "psr-4": {
            "Cognitive\\Git\\": "src/",
            "Cognitive\\Git\\Tests\\": "tests/"
        }
    }
